<root controller="SnmpParamsController">
    <ImageView name="bottomScreen" x="160" y="120"/>

    <BinaryButtonView name="menuButton" x="150" y="60" sx="0.5" sy="0.25" onClick="editSnmpVersion" />
    <TextView text="v1/2" x="135" y="53" size="0.5"/>
    <TextView text="USM" x="175" y="53" size="0.5"/>

    <TextView text="Community" x="20" y="75" size="0.75"/>
    <EditTextView x="130" y="75" width="140" height="20" length="12" onEdit="editCommunity"/>

    <TextView text="User name" x="20" y="100" size="0.75"/>
    <EditTextView x="130" y="100" width="140" height="20" length="12" onEdit="editUsername"/>

    <ButtonView name="menuButton" x="160" y="200" onClick="sendSnmp" sx="0.5" sy="0.25"/>
    <TextView text="Send" x="143" y="192" size="0.5" />

    <ButtonView name="backArrow" x="24" y="216" onClick="goBack" sx="-0.75" sy="0.75"/>

    <UpdateView onUpdate="updateRequest" />
    <ImageView name="menuButton" x="310" y="10" sx="0.25" sy="0.25"/>
</root>

